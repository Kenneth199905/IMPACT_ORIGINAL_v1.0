    "custom_liquid_AFzq8M": {
      "type": "custom-liquid",
      "name": "Custom Liquid",
      "settings": {
        "full_width": true,
        "remove_vertical_spacing": true,
        "remove_horizontal_spacing": true,
        "liquid": "<script>\n  (() => {\n    if (window.__shippingBannerBound) return;\n    window.__shippingBannerBound = true;\n\n    const DAYS = ['dim.', 'lun.', 'mar.', 'mer.', 'jeu.', 'ven.', 'sam.'];\n    const MONTHS = ['janv.', 'fév.', 'mars', 'avr.', 'mai', 'juin',\n                     'juil.', 'août', 'sept.', 'oct.', 'nov.', 'déc.'];\n\n    function getShippingDate(offset) {\n      // Date actuelle en timezone Paris\n      const now = new Date();\n      const parisNow = new Date(now.toLocaleString('en-US', { timeZone: 'Europe/Paris' }));\n      \n      // Ajouter l'offset en jours ouvrés (skip samedi/dimanche)\n      let added = 0;\n      const result = new Date(parisNow);\n      while (added < offset) {\n        result.setDate(result.getDate() + 1);\n        const day = result.getDay(); // 0=dim, 6=sam\n        if (day !== 0 && day !== 6) {\n          added++;\n        }\n      }\n      return result;\n    }\n\n    function formatDate(date) {\n      const dayName = DAYS[date.getDay()];\n      const dayNum = date.getDate();\n      const monthName = MONTHS[date.getMonth()];\n      return `${dayName} ${dayNum} ${monthName}`;\n    }\n\n    function updateBanners() {\n      document.querySelectorAll('[data-shipping-banner]').forEach(banner => {\n        const offset = parseInt(banner.dataset.daysOffset, 10) || 2;\n        const shippingDate = getShippingDate(offset);\n        const span = banner.querySelector('[data-shipping-date]');\n        if (span) {\n          span.textContent = formatDate(shippingDate);\n        }\n      });\n    }\n\n    // Mettre à jour au chargement\n    if (document.readyState === 'loading') {\n      document.addEventListener('DOMContentLoaded', updateBanners);\n    } else {\n      updateBanners();\n    }\n  })();\n</script>",
